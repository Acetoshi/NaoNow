async function getStationWaitTime(id) {
  const waitTime = await fetchStationWaitTime(id)

  console.log(waitTime)

  return waitTime;
}

async function fetchStationWaitTime(id) {
  return fetch(`https://open.tan.fr/ewp/tempsattente.json/${id}`)
    .then((response) => {
      if (!response.ok) {
        throw new Error("Network response was not ok");
      }
      return response.json();
    })
    .then((data) => {
      // Return the array of tram stations
      return data;
    })
    .catch((error) => {
      console.error(
        "Error calling https://open.tan.fr/ewp/arrets.json:",
        error
      );
      throw error;
    });
}

export default getStationWaitTime;
